#correcion

def negate(num):
    return -num

def large_num(num):
    return num > 10000

#Corregir el nombre de la variable de entrada de la funci√≥n negate
b=(input("Digite el valor que quiere cambiar"))
neg_b = negate(b)
print('b:', b, 'neg_b:', neg_b)

big = large_num(b)
print('b if big:', big)


